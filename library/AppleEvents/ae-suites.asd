(in-package :ccl)(asdf:defsystem defappleevents  :author ("Ray Pelletier")  :description "Define Appleevents"  :long-description "High level access to appleevent records"  :components ((:file "defappleevents")))(asdf:defsystem ae-suites  :author ("Terje Norderhaug")  :description "AppleEvent Suites"  :long-description "Makes MCL and its applications scriptable and recordable"  :license "LLGPL"  :serial t  :components ((:file "ae-resolve-objects")               (:file "ae-core-suite")               (:file "ae-misc-suite")               (:file "ae-recordable")               (:file "ae-text-suite")               (:file "ae-suites-sysdecl"))  :depends-on (defappleevents))