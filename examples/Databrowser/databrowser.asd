(in-package :ccl)(asdf:defsystem databrowser  :description "Databrowser classes"  :author "Shannon Spires & Terje Norderhaug"  :license "LLGPL"  :components ((:file "databrowser")))(asdf:defsystem autosize  :description "Autosize and reposition subviews when the window is resized"  :author "Shannon Spires"  :license "LLGPL"  :components ((:file "autosize")))(asdf:defsystem databrowser-mcl  :description "Modernizes the MCL IDE with the databrowser"  :author "Terje Norderhaug & Shannon Spires"  :license "LLGPL"  :components ((:file "modern-processes-inspector")               (:file "search-files-browser"))  :depends-on (databrowser autosize))(asdf:defsystem databrowser-examples  :description "Examples of using the databrowser"  :author "Shannon Spires"  :license "LLGPL"  :components ((:file "employee-browser")               (:file "file-browser-icons")               (:file "file-browser")               (:file "moons-browser"))  :depends-on (databrowser autosize)  :pathname ";Examples;")